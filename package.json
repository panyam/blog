{
  "name": "maniacalbuilder",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "start": "next dev",
    "dev": "next dev",
    "build": "rm -Rf build && next build",
    "buildall": "npm run build && npm run copystatic",
    "postbuild": "npm run sitemap && npm run rss",
    "sitemap": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-sitemap.mjs",
    "rss": "cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/generate-rss.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky install",

    "copystatic": "cp app.yaml build && cp .gcloudignore build && cp -r public build",
    "upload": "cd build ; gcloud app deploy --project maniacalbuilder --verbosity=info",
    "predeploy": "npm run buildall",
    "deploy": "npm run upload"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.15",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "contentlayer": "0.2.5",
    "esbuild": "0.17.12",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "mdx-bundler": "^9.2.1",
    "next": "12.1.6",
    "next-contentlayer": "0.2.5",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.21",
    "preact": "^10.13.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.1",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "sharp": "^0.31.3",
    "tailwindcss": "^3.2.7",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "12.1.6",
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.28",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "globby": "13.1.3",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
