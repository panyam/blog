{
  "name": "maniacalbuilder",
  "version": "1.5.3",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "devbuild": "rm -Rf build && NODE_OPTIONS=--enable-source-maps next build",
    "build": "rm -Rf build && next build",
    "start": "next start",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "buildall": "npm run build && npm run copystatic",
    "postbuild": "npm run sitemap && npm run rss",
    "rss": "ts-node -O '{\"module\": \"commonjs\"}' scripts/generate-rss.ts ",
    "sitemap": "ts-node -O '{\"module\": \"commonjs\"}' scripts/generate-sitemap.ts ",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install",
    "copystatic": "cp app.yaml build && cp .gcloudignore build && cp -r public build",
    "upload": "cd build ; gcloud app deploy --project maniacalbuilder --verbosity=info",
    "predeploy": "npm run buildall",
    "deploy": "npm run upload"
  },
  "dependencies": {
    "@fontsource/inter": "4.5.15",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next/mdx": "^13.3.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "autoprefixer": "^10.4.14",
    "axios": "^1.3.5",
    "date-fns": "^2.29.3",
    "dayjs": "^1.11.7",
    "esbuild": "0.17.16",
    "github-slugger": "^2.0.0",
    "glob": "^10.0.0",
    "gray-matter": "^4.0.3",
    "image-size": "1.0.2",
    "next": "13.3.0",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.21",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-citation": "^1.0.1",
    "rehype-highlight": "^6.0.0",
    "rehype-katex": "^6.0.2",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "^1.5.1",
    "rehype-slug": "^5.1.0",
    "remark": "^14.0.2",
    "remark-footnotes": "^4.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-prism": "^1.3.6",
    "sharp": "^0.32.0",
    "tailwindcss": "^3.3.1",
    "unist-util-visit": "^4.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.3.0",
    "@svgr/webpack": "^7.0.0",
    "@types/react": "^18.0.35",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^8.38.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "file-loader": "^6.2.0",
    "globby": "13.1.4",
    "husky": "^8.0.3",
    "inquirer": "^9.1.5",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
